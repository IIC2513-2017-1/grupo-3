<h1>Login to Eats</h1>
<div id="login-form">
  <%= form_for :session, url: session_path do |f| %>
    <div class="field">
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>
    <div class="field">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>
    <div class="actions">
      <%= f.submit 'Login' %>
    </div>
  <% end %>
</div>
<p>New to Eats? <%= link_to "Sign up now", signup_path %></p>
<p>Sell your food! <a href="/users/new/cook">Sign up as a cook</a></p>

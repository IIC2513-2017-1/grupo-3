<% content_for :title, "Previous Orders" %>
<h1 id="view-subtitle" align="center">Orders Historial</h1>

<h3>Orders</h3>
<ul>
  <% orders.each do |order| %>
    <li>
      <%=
        link_to(
          "Order #{order.created_at.strftime("%F")}",
          order_path(order)
        )
      %>
    </li>
  <% end %>
</ul>
